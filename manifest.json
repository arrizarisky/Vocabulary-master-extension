{
  "manifest_version": 3,
  "name": "Vocab Master Pro",
  "version": "1.0",
  "description": "Save and manage English vocabulary with manual add",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
